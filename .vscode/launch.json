 {
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Console App",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "program": "${file}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            },
            "envFile": "${workspaceRoot}/.env",
            "console":"integratedTerminal"
        }
    ]
 }